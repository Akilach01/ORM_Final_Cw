<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="riviewAuthPane" prefHeight="550.0" prefWidth="806.0" style="-fx-background-color: #fff;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.orm.controller.RicieptionAuthController">
   <children>
      <ImageView fitHeight="95.0" fitWidth="130.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true">
         <image>
            <Image url="@../assests/pngtree-mental-health-problems-flat-illustration.png" />
         </image>
      </ImageView>
      <Label layoutX="255.0" layoutY="123.0" text="Enter your Credentials" textFill="#333333">
         <font>
            <Font name="Gabarito Regular" size="36.0" />
         </font>
      </Label>
      <Label layoutX="192.0" layoutY="167.0" text="Log in to manage patient appointments, records, and therapist schedules." textFill="#333333">
         <font>
            <Font name="Gabarito Regular" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="emailTxt" layoutX="271.0" layoutY="240.0" prefHeight="39.0" prefWidth="262.0" promptText="sample@gmail.com" style="-fx-padding: 0 15px; -fx-background-color: #fafafa;" />
      <Label layoutX="271.0" layoutY="220.0" text="Gmail Address" textFill="#333333">
         <font>
            <Font name="Gabarito Regular" size="13.0" />
         </font>
      </Label>
      <Label layoutX="271.0" layoutY="300.0" text="Password" textFill="#333333">
         <font>
            <Font name="Gabarito Regular" size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwotdTxt" layoutX="271.0" layoutY="319.0" prefHeight="39.0" prefWidth="262.0" promptText="* * * * * * *" style="-fx-padding: 0 15px; -fx-background-color: #fafafa;" />
      <JFXButton layoutX="272.0" layoutY="381.0" onAction="#authRicieption" prefHeight="39.0" prefWidth="262.0" style="-fx-background-color: #0d6efd;" text="Authenticate " textFill="#fcfcfc">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <JFXButton layoutX="47.0" layoutY="491.0" onAction="#backLanding" text="Back">
         <font>
            <Font name="Segoe UI Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <Label layoutX="307.0" layoutY="435.0" prefHeight="19.0" prefWidth="138.0" text="don't have an account ?" textFill="#333333">
         <font>
            <Font name="Gabarito Regular" size="13.0" />
         </font>
      </Label>
      <Label layoutX="457.0" layoutY="435.0" onMouseClicked="#gotoRegister" text="Sign Up " textFill="#0d6efd">
         <font>
            <Font name="Gabarito Regular" size="13.0" />
         </font>
      </Label>
      <Label layoutX="47.0" layoutY="92.0" text="MindVista">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
